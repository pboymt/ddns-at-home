{
    "$schema": "./settings.schema.json",
    "version": 2,
    "global": {
        "disableIPv4": false,
        "disableIPv6": false,
        "TTL": 600
    },
    "services": [
        {
            "name": "aliyun",
            "apiKey": "<aliyun api key>",
            "apiSecret": "<aliyun api secret>",
            "domains": [
                {
                    "domainName": "example.com",
                    "resourceRecords": [
                        {
                            "resourceRecord": "home",
                            "disableIPv4": false,
                            "disableIPv6": true,
                            "TTL": 600
                        }
                    ]
                }
            ]
        }
    ],
    "schedule": {
        "immediate": false
    }
}